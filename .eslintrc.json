{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "max-len": [
        "error", 120
      ],
      "object-curly-spacing": [
        "error", "always"
      ],
      "space-infix-ops": "error"
    }
  }
